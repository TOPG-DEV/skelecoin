{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/assets/skelecoin.jpg",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/assets/skelecoin.jpg",
      "dest": "/public/assets/skelecoin.jpg"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
